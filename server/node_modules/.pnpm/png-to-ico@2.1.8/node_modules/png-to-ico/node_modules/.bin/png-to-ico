#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules/png-to-ico/bin/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules/png-to-ico/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules/png-to-ico/bin/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules/png-to-ico/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/png-to-ico@2.1.8/node_modules:/mnt/c/Users/Arcanaz/Downloads/Programming/Projects/ImageConverter/server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
